{
  "name": "contao-manager-api-browser",
  "version": "1.0.0",
  "description": "A web-based interface for managing multiple Contao Manager instances through their APIs",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrflory/contao-manager-api-browser.git"
  },
  "homepage": "https://github.com/mrflory/contao-manager-api-browser#readme",
  "bugs": {
    "url": "https://github.com/mrflory/contao-manager-api-browser/issues"
  },
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "nodemon --ignore data/config.json server.js",
    "dev:react": "vite",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:react\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:react19-compat": "node scripts/test-react19-compat.js",
    "test:chakra-v3-compat": "node scripts/test-chakra-v3-compat.js",
    "test:build": "npm run build && npm run preview",
    "mock:server": "ts-node src/test/mockServer/start.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.21.0",
    "@emotion/react": "^11.14.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^5.0.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.5.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "keywords": [
    "contao",
    "manager",
    "api",
    "browser",
    "dashboard",
    "multi-site",
    "oauth",
    "cms",
    "monitoring"
  ],
  "author": "Florian Stallmann <florian@stallmann.me>",
  "license": "MIT"
}
